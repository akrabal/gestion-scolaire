<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="A fully featured admin theme which can be used to build CRM, CMS, etc.">
    
    <!-- Favicon -->
    <link rel="shortcut icon" href="static/dashkit/assets/favicon/favicon.ico" type="image/x-icon">
    
    <!-- Map CSS -->
    <link rel="stylesheet" href="static/dashkit/mapbox-gl-js/v0.53.0/mapbox-gl.css">
    
    <!-- Libs CSS -->
    <link rel="stylesheet" href="static/dashkit/assets/css/libs.bundle.css">
    
    <!-- Theme CSS -->
    <link rel="stylesheet" href="static/dashkit/assets/css/theme.bundle.css" id="stylesheetLight">
    <link rel="stylesheet" href="static/dashkit/assets/css/theme-dark.bundle.css" id="stylesheetDark">
    
    <style>body { display: none; }</style>
    
    <!-- Title -->
    <title>Dashkit</title>
    
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async="" src="static/dashkit/gtag/js.js?id=UA-156446909-1"></script><script>window.dataLayer = window.dataLayer || [];function gtag(){dataLayer.push(arguments);}gtag("js", new Date());gtag("config", "UA-156446909-1");</script>
  </head>
  <body>
  <!-- MAIN CONTENT -->
  <div class="main-content"> 

<div class="row justify-content-center">
    <div class="col-12 col-lg-10 col-xl-8">

      <!-- Header -->
     

      <!-- Content -->
      <div class="card card-body p-5">
        <div class="row">
          <div class="col text-end">

          

          </div>
        </div> <!-- / .row -->
        <div class="row">
          <div class="col text-center">


            <!-- Title -->
            <h2 class="mb-2">
             BULLETIN
            </h2>

           

          </div>
        </div> <!-- / .row -->
        <div class="row">
          <div class="col-12 col-md-6">

           

            <!-- Text -->
            <p class="text-muted mb-4">
              <strong class="text-body"><%= etablisements.NomEtab  %> </strong> <br>
              <%= etablisements.adresseEtab  %> <br>
              <time datetime="2018-04-23">Apr 23, 2018</time>
            </p>
           

           

          </div>
          <div class="col-12 col-md-6 text-md-end">

           

            <!-- Text -->
            <p class="text-muted mb-4">
              <strong class="text-body"><%= user.Eleve.NomEleve  %> <%=  user.Eleve.prenomEleve  %></strong> <br>
              classe:  <%= classe[0].niveauclasse  %>  <br>
            </p>

            

          </div>
        </div>
        <div class="row">
          <div class="col-12">

            <!-- Table -->
            <div class="table-responsive">
              <table class="table my-4">
                <thead>
                  <tr>
                    <th class="px-0 bg-transparent border-top-0">
                      <span class="h6">Matieres</span>
                    </th>
                  
                    <th class="px-0 bg-transparent border-top-0 text-end">
                      <span class="h6">Notes</span>
                    </th>
                  </tr>
                </thead>
                <tbody>
                <%for(i=0;i<matier.length;i++){ %>
                  <tr>
                    <td class="px-0">
                     <%= matier[i].NomMatiere  %> 
                    </td>
                   
                    <td class="px-0 text-end">
                     <%=notes[i][0].valenote  %> 
                    </td>
                  </tr>
                <% } %> 
                  
                  <tr>
                    <td class="px-0 border-top border-top-2">
                      <strong>Moyenne Des Notes</strong>
                    </td>
                    <td colspan="2" class="px-0 text-end border-top border-top-2">
                      <% let cool=0; for(i=0;i<matier.length;i++){ cool =cool+notes[i][0].valenote  %>
                        <% if(i == matier.length-1){ %> 
                        <span class="h3">
                          <%= cool/matier.length  %> 
                        </span>
                        <% } %> 
                      <% } %> 
                     
                    </td>
                  </tr>
                </tbody>
              </table>
            </div>

            

          </div>
        </div> <!-- / .row -->
      </div>

    </div>
  </div>
  <%- include ('.././include/footerdash')%> 