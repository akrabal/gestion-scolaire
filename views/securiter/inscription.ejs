

   
<%- include ('../include/header')%> 
  

   <!-- CONTENT -->
   <section class="section-border border-primary">
    <div class="container d-flex flex-column">
      <div class="row align-items-center justify-content-center gx-0 min-vh-100">
         <div class="col-12 col-md-5 col-lg-4 order-md-1 mb-auto mb-md-0 pb-8 py-md-11" >
          <!-- Heading -->
          <h1 class="mb-0 fw-bold text-center ">
           inscrire
          </h1>

          <!-- Form -->
          <form class="mb-6" method="POST" action="/inscription/ajoutdirecteurIns">

            <div class="form-group">
              <label class="form-label" for="email">
               Nom Etablisement
              </label>
              <input type="text" name="nomEatb" class="form-control" id="" placeholder="Nom de votre Etablisement">
            </div> 

            <div class="form-group">
              <label class="form-label" for="email">
                Adresse de l'etablisement
              </label>
              <input type="text" class="form-control" name="adresseEtab"  placeholder="adresse de l'etablisement"  id="">
            </div>

            <div class="form-group">
              <label class="form-label" for="email">
                Numero de telephone de l'etablisement
              </label>
              <input type="number" name="numtelEtab" class="form-control" placeholder="numero de telephone de l'etablisement" id="">
            </div>

            <div class="form-group">
              <label class="form-label" for="email">
               Nom du directeur
              </label>
              <input type="text" class="form-control" name="nom" id="" placeholder="Nom du directeur ">
            </div>

            <div class="form-group">
              <label class="form-label" for="email">
                Prenom du Directeur
              </label>
              <input type="text" class="form-control" name="prenom" id="" placeholder="Prenom du directeur ">
            </div>

            <div class="form-group">
              <label class="form-label" for="email">
                mots de passe utilisateur
              </label>
              <input type="password" class="form-control" name="password" placeholder="mots de passe utilisateur" id="">
            </div>

            <div class="form-group">
              <label class="form-label" for="email">
                email du directeur
              </label>
              <input type="email" class="form-control" name="email" id="" placeholder="email du directeur">
            </div>

            <div class="form-group">
              <label class="form-label" for="email">
                numero de telephone du directeur
              </label>
              <input type="number" class="form-control" name="numtel" placeholder="numero de telephone du directeur" id="">
            </div>


            <!-- Submit -->
            <button class="btn w-100 btn-primary" type="submit">
              Sign in
            </button>

          </form>

         

        </div>
    

      </div> <!-- / .row -->
    </div> <!-- / .container -->
  </section>


    
<%-include('../include/footer') %> 
    


   

   